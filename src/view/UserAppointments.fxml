      <TableView fx:id="appointmentsTable" layoutX="10.0" layoutY="119.0" prefHeight="311.0" prefWidth="680.0">
        <columns>
          <TableColumn fx:id="appointmentIDColumn" prefWidth="75.0" text="Apt. ID" />
          <TableColumn fx:id="titleColumn" prefWidth="143.0" text="Title" />
            <TableColumn fx:id="typeColumn" prefWidth="123.0" text="Type" />
            <TableColumn fx:id="startColumn" prefWidth="169.0" text="Start" />
            <TableColumn fx:id="endColumn" prefWidth="170.0" text="End" />
        </columns>
      </TableView>
      <Button layoutX="252.0" layoutY="81.0" mnemonicParsing="false" onAction="#showAppointmentsButton" text="Show Appointments For This User" />
